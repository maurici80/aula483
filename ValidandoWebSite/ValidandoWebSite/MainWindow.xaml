<Window x:Class="ValidandoWebSite.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ValidandoWebSite"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="tbxUrl" Margin="0,39,30,340" TextWrapping="Wrap" HorizontalAlignment="Right" Width="732"/>
        <Button x:Name="button" Click="Button_Click" Content="Button" HorizontalAlignment="Right" Height="42" Margin="0,116,30,0" VerticalAlignment="Top" Width="732"/>
        <TextBox x:Name="tbxStatusReport" Margin="0,0,30,52" TextWrapping="Wrap" HorizontalAlignment="Right" Width="732" Height="159" VerticalAlignment="Bottom"/>



    </Grid>
</Window>
